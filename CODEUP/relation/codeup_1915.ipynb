    "\n",
    "def fibo(n):\n",
    "    if n==1 or n==2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(n-1)+fibo(n-2)\n",
    "        \n",
    "n = int(input())\n",
    "result = fibo(n)\n",
    "print(result)"
